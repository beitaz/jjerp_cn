<%= simple_form_for(@expend) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :reason %>
    <%= f.input :amount %>
    <%= f.input :creator %>
    <%= f.input :note %>
    <%= f.input :deleted %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
