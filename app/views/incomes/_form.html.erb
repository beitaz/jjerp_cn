<%= simple_form_for(@income) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :origin %>
    <%= f.input :origin_id %>
    <%= f.input :target %>
    <%= f.input :amount %>
    <%= f.input :creator %>
    <%= f.input :note %>
    <%= f.input :deleted %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
